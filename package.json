{
  "name": "flexsass",
  "version": "1.0.0",
  "description": "Mscit It Sem-2 FrontEnd Project",
  "main": "gulpfile_.js",
  "scripts": {
    "sass": "node-sass src/sass/main.scss dist/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass src/sass/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipenparmar12/flexSass.git"
  },
  "author": "Dipends",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dipenparmar12/flexSass/issues"
  },
  "homepage": "https://github.com/dipenparmar12/flexSass#readme",
  "devDependencies": {
    "node-sass": "^4.11.0",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
